pipeline{
    
    agent any
    stages{
        
        stage("Build"){
            steps{
            echo("build the project")
            }
        }
        
        stage("Deploy on dev env"){
            steps{
            echo("running test on dev env")
            }
        }
        
        stage("Deploy on dev qa"){
            steps{
            echo("running test on qa env")
            }
        }
    }
}